{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://cufo.cc" },
        { "url": "https://cufo.cc/**" },
        { "url": "https://gateway.ddot.cc" },
        { "url": "https://gateway.ddot.cc/**" }
      ]
    }
  ]
}
